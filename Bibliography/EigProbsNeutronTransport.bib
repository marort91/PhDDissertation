@article{montagnini_time-dependent_1971,
	title = {The time-dependent rectilinear transport equation},
	volume = {1},
	issn = {0041-1450},
	url = {http://dx.doi.org/10.1080/00411457108231437},
	doi = {10.1080/00411457108231437},
	abstract = {The rectilinear or rod model of neutron transport consists of a segment which is assumed to be capable of transporting particles. Such particles (neutrons) can move only to the right or to the left on the segment at a constant speed, and suffer scattering, capture and fission. We have shown that the time-eigenvalue spectrum of this model is made of a finite, non-void set of real eigenvalues, plus infinitely many complex eigenvalues, distributed in pairs of complex conjugates. A simple asymptotic formula for the high-order complex eigenvalues has been obtained. As an application, we have considered the following initial value problem: find the outgoing density of neutrons, at one end of the rod, at any time after the injection of a neutron pulse at the same end. A complete series expansion of the outgoing density has been given. The successive time-derivatives of this expansion show certain characteristic discontinuities which are due to the motion, from one end of the rod to the other, of discontinuities (wave fronts) of the time-dependent neutron population. This suggests that the imaginary parts of the complex eigenvalues have just the task of representing the oscillatory motion of these wave fronts.},
	number = {1},
	urldate = {2016-03-09},
	journal = {Transport Theory and Statistical Physics},
	author = {Montagnini, B. and Pierpaoli, V.},
	month = jan,
	year = {1971},
	pages = {59--75},
	file = {Full Text PDF:/Users/mario/Library/Application Support/Zotero/Profiles/x87drm79.default/zotero/storage/IJJ95SDP/Montagnini and Pierpaoli - 1971 - The time-dependent rectilinear transport equation.pdf:application/pdf;Snapshot:/Users/mario/Library/Application Support/Zotero/Profiles/x87drm79.default/zotero/storage/3QQIWZ6A/00411457108231437.html:text/html}
}

