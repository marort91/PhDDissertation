	author = {Kópházi, József and Lathouwers, Danny},
	month = dec,
	year = {2012},
	keywords = {ADS, Alpha eigenvalue, Discrete ordinate, Transport},
	pages = {167--174},
	file = {ScienceDirect Full Text PDF:/Users/mario/Library/Application Support/Zotero/Profiles/x87drm79.default/zotero/storage/VDJBPNSS/Kópházi and Lathouwers - 2012 - Three-dimensional transport calculation of multipl.pdf:application/pdf;ScienceDirect Snapshot:/Users/mario/Library/Application Support/Zotero/Profiles/x87drm79.default/zotero/storage/UBRNNKWJ/S0306454912002344.html:text/html}
}

